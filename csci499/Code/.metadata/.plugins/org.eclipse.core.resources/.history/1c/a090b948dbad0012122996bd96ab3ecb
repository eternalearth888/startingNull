package game;

import java.awt.*;

public class MainScreen {
	public Background background;
	
	public MainScreen(Game play) {
		background = play.getBackground();
	}
	
	public void paintComponent(Graphics g) {
		super.paintComponent(g);
		raphics2D g2 = (Graphics2D) g;
		
		background.draw(g2);
	}
}
