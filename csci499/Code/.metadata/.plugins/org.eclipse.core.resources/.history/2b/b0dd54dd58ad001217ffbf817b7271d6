package game;

import java.util.ArrayList;

public class Trainer {
	private Game game;
	private ArrayList<Integer> projPath;
	private double angle;
	private int power;
	private final int SIZE = 5;

	public Trainer(Game g) {
		this.game = g;
		angle = 45;
		power = 50;
		projPath = new ArrayList<Integer>();
	}
}
