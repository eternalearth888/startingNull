package exercises;

import exercises.Node;

public class LinkedList {
	
	// Fields
	private int data;
	private LinkedList head;
	
	// Default constructor for Linkedlist
	public LinkedList(int modifyData) {
		data = modifyData;
	}

	// Getters and Setters
	
	public int getData() {
		return data;
	}

	public void setData(int data) {
		this.data = data;
	}

	public LinkedList getHead() {
		return head;
	}

	public void setHead(LinkedList head) {
		this.head = head;
	}
	
	
	
}
